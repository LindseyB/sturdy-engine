{
  "name": "sturdy-engine",
  "version": "1.0.0",
  "description": "An electron app for generating gifs",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "start:dev": "cross-env ENVIRONMENT=dev electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "webpack",
    "build:watch": "webpack-dev-server --hot",
    "lint": "eslint \"src/*.js\"",
    "lint:fix": "eslint --fix \"src/*.js\"",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky install",
    "dev": "concurrently \"npm run start:dev\" \"npm run build:watch\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "https://github.com/LindseyB/sturdy-engine",
  "keywords": [
    "gif"
  ],
  "lint-staged": {
    "*.js": "eslint --fix src/*.js",
    "**/*": "prettier --write --ignore-unknown"
  },
  "author": "LindseyB",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@storybook/addon-actions": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/builder-webpack5": "^8.3.2",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.3.2",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "electron": "^32.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "sass": "^1.79.2",
    "sass-loader": "^16.0.2",
    "ts-loader": "^9.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "babel-loader": "^9.2.1",
    "bulma": "^1.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron-rebuild": "^3.2.9",
    "electron-squirrel-startup": "^1.0.1",
    "ffmpeg-static": "^5.2.0",
    "node-sass": "^9.0.0",
    "react": "^18.3.1",
    "react-bulma-components": "^4.1.0",
    "react-dom": "^18.3.1",
    "srt-parser-2": "^1.2.3",
    "style-loader": "^4.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
